<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>ros2_package</name>
  <version>0.0.1</version>
  <description>The ROS 2 package template</description>
  <maintainer email="pawel.irzyk6@gmail.com">Pawe≈Ç Irzyk</maintainer>
